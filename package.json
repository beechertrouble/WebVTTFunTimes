{
  "name": "WebVTTFunTimes",
  "version": "0.1.0",
  "description": "handy helper for making .vtt track files for html5 videos",
  "repository": {
    "type": "git",
    "url": "https://github.com/beechertrouble/WebVTTFunTimes"
  },
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package": "asar pack WebVTTFunTimes-darwin-x64/WebVTTFunTimes.app/Contents/Resources/app WebVTTFunTimes-darwin-x64/WebVTTFunTimes.app/Contents/Resources/app.asar && rm -rf WebVTTFunTimes-darwin-x64/WebVTTFunTimes.app/Contents/Resources/app",
    "build": "electron-packager . WebVTTFunTimes --overwrite --ignore=fe-src --ignore=node_modules/electron-* --ignore=.gitignore --ignore=Gruntfile.js --ignore=README.md --ignore=package-lock.json --ignore=node_modules/grunt-cli"
  },
  "author": "beechbot",
  "license": "MIT",
  "devDependencies": {
    "asar": "^1.0.0",
    "electron": "^4.1.0",
    "electron-packager": "^13.1.1",
    "grunt": "latest",
    "grunt-contrib-clean": "latest",
    "grunt-contrib-concat": "latest",
    "grunt-contrib-jshint": "latest",
    "grunt-contrib-less": "latest",
    "grunt-contrib-uglify": "latest",
    "grunt-contrib-watch": "latest"
  },
  "dependencies": {}
}
